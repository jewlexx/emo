{
  "name": "emo-react",
  "version": "1.1.3",
  "description": "Universal emojis for React that appear the same on every device",
  "repository": "jamesinaxx/emo-react",
  "license": "MIT",
  "author": "jamesinaxx",
  "main": "./dist/index.js",
  "source": "./src/index.tsx",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx rimraf dist && rollup --config rollup.config.js",
    "predeploy": "cd demo && yarn install && yarn run build",
    "deploy": "npx gh-pages -d demo/build",
    "dev": "npx rollup --config rollup.config.js --watch",
    "prepack": "yarn build"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "8.3.0",
    "@types/react": "17.0.37",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "eslint": "8.4.0",
    "eslint-config-airbnb": "19.0.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "prettier": "2.5.1",
    "rollup-plugin-terser": "7.0.2",
    "tslib": "2.3.1",
    "typescript": "4.5.2"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "engines": {
    "node": ">=10"
  },
  "packageManager": "yarn@3.1.1"
}
